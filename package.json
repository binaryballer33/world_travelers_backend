    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.0",
    "resend": "^3.2.0",
    "stripe": "^14.18.0",
    "supertest": "^6.3.4",
    "zod": "^3.22.4"
